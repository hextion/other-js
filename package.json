    "eslint-plugin-jsdoc": "^15.12.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "1.18.2"
  },
  "dependencies": {
    "assert": "^2.0.0"
  }
